<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jasperTemplate PUBLIC "-//JasperReports//DTD Template//EN" "http://jasperreports.sourceforge.net/dtds/jaspertemplate.dtd">

<jasperTemplate>
	<style name="Title" forecolor="#000000" backcolor="#FFFFFF" hAlign="Left" vAlign="Middle" fontSize="10" isBold="true" pdfFontName="Helvetica-Bold" isPdfEmbedded="true" pdfEncoding="Cp1252" />
	<style name="Page" forecolor="#000000" backcolor="#FFFFFF" hAlign="Left" vAlign="Middle" fontSize="9" isBold="false" pdfFontName="Helvetica" isPdfEmbedded="true" pdfEncoding="Cp1252" />
	<style name="PageBold" style="Page" isBold="true" pdfFontName="Helvetica-Bold" />
	
	<style name="Body" isDefault="true" forecolor="#000000" backcolor="#FFFFFF" hAlign="Left" vAlign="Middle" fontSize="8" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="Helvetica" isPdfEmbedded="true" pdfEncoding="Cp1252" />
