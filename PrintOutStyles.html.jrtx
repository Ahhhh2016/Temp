	<style name="PaymentLabel" style="Body" forecolor="#FF0000" fontSize="9" isBold="true"/>
		
	<style name="Footer" style="Body" />
	<style name="Summary" style="Body" fontSize ="9" />
	<style name="SummaryBold" style="Body" fontSize="9" isBold="true" pdfFontName="Helvetica-Bold" />
	
	<style name="Dashed" forecolor="#000000" backcolor="#FFFFFF" hAlign="Left" vAlign="Middle" fontSize="8" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="true" pdfFontName="Helvetica" isPdfEmbedded="true" pdfEncoding="Cp1252" />
</jasperTemplate>
